	local CloseButton = Instance.new("TextButton")
	CloseButton.Parent = screenGui
	CloseButton.Size = UDim2.new(0.038, 0,0.092, 0)
	CloseButton.Position = UDim2.new(0.365, 0,0.123, 0)
	CloseButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	CloseButton.Text = "X"
	CloseButton.TextScaled = true
	CloseButton.Font = Enum.Font.GothamBold
	CloseButton.TextColor3 = Color3.fromRGB(0, 0, 0)
	local closebuttoncorner = Instance.new("UICorner")
	closebuttoncorner.Parent = CloseButton
	closebuttoncorner.CornerRadius = UDim.new(0.5)

	local closebuttonstroke = Instance.new("UIStroke")
	closebuttonstroke.Parent = CloseButton
	closebuttonstroke.Thickness = 3
	closebuttonstroke.Color = Color3.fromRGB(0, 0, 0)	
	closebuttonstroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
	local dragging
	local dragInput
	local dragStart
	local startPos

	local function updatePosition(input)
		local delta = input.Position - dragStart
		CloseButton.Position = UDim2.new(
			startPos.X.Scale,
			startPos.X.Offset + delta.X,
			startPos.Y.Scale,
			startPos.Y.Offset + delta.Y
		)
	end

	CloseButton.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			dragStart = input.Position
			startPos = CloseButton.Position

			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)

	CloseButton.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseMovement then
			dragInput = input
		end
	end)

	game:GetService("UserInputService").InputChanged:Connect(function(input)
		if dragging and input == dragInput then
			updatePosition(input)
		end
	end)


	local draggablebar = Instance.new("TextLabel")
	draggablebar.Parent = scrollingFrame
	draggablebar.Position = UDim2.new(0.0001,0,0.0001,0)
	draggablebar.Size = UDim2.new(1,0,0.065,0)
	draggablebar.Text = ""
	draggablebar.Transparency = 1
	local UserInputService = game:GetService("UserInputService")

	local dragging
	local dragInput
	local dragStart
	local scrollingStartPos = scrollingFrame.Position
	local backgroundStartPos = backgroundImage.Position

	local function updatePosition(input)
		local delta = input.Position - dragStart
		local newScrollingPos = UDim2.new(
			scrollingStartPos.X.Scale,
			scrollingStartPos.X.Offset + delta.X,
			scrollingStartPos.Y.Scale,
			scrollingStartPos.Y.Offset + delta.Y
		)
		local newBackgroundPos = UDim2.new(
			backgroundStartPos.X.Scale,
			backgroundStartPos.X.Offset + delta.X,
			backgroundStartPos.Y.Scale,
			backgroundStartPos.Y.Offset + delta.Y
		)
		scrollingFrame.Position = newScrollingPos
		backgroundImage.Position = newBackgroundPos
	end

	draggablebar.Active = true  -- Make sure it receives input!

	draggablebar.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			dragStart = input.Position
			scrollingStartPos = scrollingFrame.Position
			backgroundStartPos = backgroundImage.Position

			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)

	draggablebar.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseMovement then
			dragInput = input
		end
	end)

	UserInputService.InputChanged:Connect(function(input)
		if dragging and input == dragInput then
			updatePosition(input)
		end
	end)
-- Grab the CloseButton
local hidescreengui = false

CloseButton.MouseButton1Click:Connect(function()
     scrollingFrame.Visible = not scrollingFrame.Visible
     backgroundImage.Visible = not backgroundImage.Visible
     if scrollingFrame.Visible == false then
         hidescreengui = true
     else
         hidescreengui = false
     end
end)
