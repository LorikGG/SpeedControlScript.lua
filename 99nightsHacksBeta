	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local hrp = character:WaitForChild("HumanoidRootPart")
	local playerGui = player:WaitForChild("PlayerGui")
	local humanoid = character:WaitForChild("Humanoid")
	local itemsFolder = workspace:WaitForChild("Items")
	-----------------------------------------------------------------------
	-- ðŸ”§ Ensure every model has a PrimaryPart
	for _, model in ipairs(itemsFolder:GetChildren()) do
		if model:IsA("Model") and not model.PrimaryPart then
			for _, part in ipairs(model:GetChildren()) do
				if part:IsA("BasePart") then
					model.PrimaryPart = part
					break
				end
			end
		end
	end
	-----------------------------------------------------------------------
	-- Create ScreenGui
	local screenGui = Instance.new("ScreenGui")
	screenGui.Parent = playerGui
	screenGui.IgnoreGuiInset = true
	screenGui.Name = "screengui"
	-----------------------------------------------------------------------
	-- Background Image
	local backgroundImage = Instance.new("ImageLabel")
	backgroundImage.Parent = screenGui
	backgroundImage.Size = UDim2.new(0.262, 0, 0.65, 0)
	backgroundImage.Position = UDim2.new(0.124, 0, 0.175, 0)
	backgroundImage.Image = "rbxassetid://110415478569986"
	backgroundImage.BackgroundTransparency = 1
	-----------------------------------------------------------------------
	local backgroundCorner = Instance.new("UICorner")
