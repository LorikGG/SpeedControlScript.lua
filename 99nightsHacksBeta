	-- Store reference to the frame
	local fuelFrame = nil

	FuelForFireMenu.MouseButton1Click:Connect(function()----------------------------------------------------------------------------------------------------------------------------------------------
		if not fuelFrame then
			-- Only create once
			fuelFrame = Instance.new("Frame")
			fuelFrame.Name = "FuelFrame"
			fuelFrame.Parent = screenGui
			fuelFrame.Size = UDim2.new(0.379, 0, 0.638, 0)
			fuelFrame.Position = UDim2.new(0.398, 0, 0.187, 0)
			fuelFrame.BackgroundColor3 = Color3.fromRGB(83, 101, 115)
			fuelFrame.BackgroundTransparency = 0.3
			 

			local metalstroke = Instance.new("UIStroke")
			metalstroke.Parent = fuelFrame
			metalstroke.Thickness = 3
			local BackgroundFuel = Instance.new("ImageLabel")
			BackgroundFuel.Name = "FuelFrame"
			BackgroundFuel.Parent = fuelFrame
			BackgroundFuel.Size = UDim2.new(1, 0, 1, 0)
			BackgroundFuel.Position = UDim2.new(0.0001, 0, 0.0001, 0)
			BackgroundFuel.BackgroundColor3 = Color3.fromRGB(83, 101, 115)
			BackgroundFuel.ImageTransparency = 0.5

			BackgroundFuel.Image = "rbxassetid://93629345963121"
			BackgroundFuel.Transparency = 1

			local fueltitle = Instance.new("TextLabel")
			fueltitle.Parent = fuelFrame
